{
  "name": "@sar-creator/tex-packer",
  "displayName": "SAR Creator Texture Packer",
  "version": "0.3.5",
  "description": "Free texture packer core",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "tsc && yarn esbuild && yarn copy-templates",
    "esbuild": "esbuild src/index.ts --bundle --tsconfig=tsconfig.json --platform=node --outfile=dist/index.js",
    "copy-templates": "cp src/exporters/*.mst esm/exporters/ && cp src/exporters/*.mst dist/exporters/",
    "example:basic": "node examples/basic/index.js"
  },
  "keywords": [
    "texture",
    "packer",
    "gulp",
    "gulpjs",
    "gulpplugin",
    "texturepacker",
    "texture-packer",
    "sprites",
    "spritesheet",
    "export",
    "sprite",
    "2d"
  ],
  "authors": "Alexander Norinchak",
  "contributors": [
    "wangweihua@bytedance.com"
  ],
  "repository": {
    "type": "git",
    "url": "git@code.byted.org:cg-lab/sar-creator-text-packer.git"
  },
  "license": "MIT",
  "dependencies": {
    "@jvitela/mustache-wax": "^1.0.1",
    "jimp": "0.22.8",
    "maxrects-packer": "^2.5.0",
    "mustache": "^2.3.0",
    "tinify": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.0",
    "esbuild": "^0.18.11",
    "typescript": "^5.1.6"
  }
}
